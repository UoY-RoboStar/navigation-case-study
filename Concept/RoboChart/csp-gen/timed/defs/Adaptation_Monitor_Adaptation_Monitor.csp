			)
		
		Stateful(id__) = 
			((let
				getsetLocalChannels = {|get_outputData,set_outputData,
				get_dataToSend,set_dataToSend,
				get_laserScan_data,set_laserScan_data,
				get_laserScan_flag,set_laserScan_flag,
